{
	"name": "obsidian-sample-plugin",
	"version": "0.0.1",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:*",
		"dev:esbuild": "node esbuild.config.mjs",
		"dev:css": "tailwindcss -i ./src/styles.css -o ./styles.css --watch",
		"build": "npm-run-all --parallel build:*",
		"build:esbuild": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:css": "tailwindcss -i ./src/styles.css -o ./styles.css --minify",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/node": "^18.0.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "^10.4.21",
		"builtin-modules": "3.3.0",
		"esbuild": "0.19.0",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tslib": "2.4.0",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@langchain/anthropic": "^0.3.26",
		"@langchain/core": "^0.3.71",
		"@langchain/deepseek": "^0.1.0",
		"@langchain/openai": "^0.6.7",
		"@radix-ui/react-checkbox": "^1.3.2",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-slider": "^1.3.6",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@types/uuid": "^10.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"langchain": "^0.3.30",
		"lucide-react": "^0.539.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tailwind-merge": "^3.3.1",
		"uuid": "^11.1.0"
	},
	"overrides": {
		"zod": "3.25.67"
	}
}
